<Map background-color="#00000000" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">

  <Style name="tree">
    <Rule>
      <MarkersSymbolizer allow-overlap="true" fill="#009900" height="5" width="5"/>
    </Rule>
  </Style>

  <Layer name="trees" srs="+proj=longlat +datum=WGS84">
    <StyleName>tree</StyleName>
    <Datasource>
      <Parameter name="dbname">trees_api</Parameter>
      <Parameter name="table">( SELECT location FROM api_tree WHERE current_propertyset_id IS NOT NULL) AS data</Parameter>
      <Parameter name="type">postgis</Parameter>
      <Parameter name="user">trees</Parameter>
      <Parameter name="estimate_extent">false</Parameter>
    </Datasource>
  </Layer>
</Map>
